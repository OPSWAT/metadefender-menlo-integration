  "version": "0.2.0",
  "configurations": [
    {
      // run in root
      // python3 -m venv .venv
      // python3 -m pip install -r requirements.txt
      "name": "Python: Menlo",
      "type": "python",
      "request": "launch",
      "module": "metadefender_menlo",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "Node Project",
      "program": "${workspaceFolder}/nodejs/index.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "env": {
        // "SERVER_HOST": "api-qa.metadefender.com",
        // "SERVER_PORT": "443",
      }
    }
  ]
}
